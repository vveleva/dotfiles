{
  "manifest_version": 2,

  "name": "Editable Content",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "css": ["content_script.css"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
      "default_icon": "icon.png",
      "default_title": "icon"
  },
  "permissions": ["*://*/*"]
}
